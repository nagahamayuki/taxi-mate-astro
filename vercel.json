{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.taxi-mate.jp"
        }
      ],
      "destination": "https://taxi-mate.jp/:path*",
      "permanent": true
    },
    {
      "source": "/:path*/index.html",
      "destination": "/:path*/",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path((?!index$|index\\.html$|.*\\/|$|.*\\.xml$|robots\\.txt$).+)",
      "destination": "/:path/",
      "permanent": true
    },
    {
      "source": "/job",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/job/index.html",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/registration",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/registration/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/sitemap.xml/",
      "destination": "/sitemap.xml",
      "permanent": true
    },
    {
      "source": "/job/01/",
      "destination": "/jobs/0ed9ca40-2352-4082-8c0b-6d284dd8a7e8/",
      "permanent": true
    },
    {
      "source": "/job/02/",
      "destination": "/jobs/a8b6b470-988d-4721-b071-c02404d12578/",
      "permanent": true
    },
    {
      "source": "/job/03/",
      "destination": "/jobs/19282f2e8b7848adb3aea7f62b844b7a/",
      "permanent": true
    },
    {
      "source": "/job/04/",
      "destination": "/jobs/1bb06ca6-b07b-404d-bb7f-c51e4c35449a/",
      "permanent": true
    },
    {
      "source": "/job/05/",
      "destination": "/jobs/0fdf0a54-ce2c-4b5e-8240-325fd8bfd269/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    }
  ],
  "trailingSlash": true,
  "cleanUrls": false
}

{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.taxi-mate.jp"
        }
      ],
      "destination": "https://taxi-mate.jp/:path*",
      "permanent": true
    },
    {
      "source": "/:path*/index.html",
      "destination": "/:path*/",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path((?!index$|index\\.html$|.*\\/|.*\\.(xml|txt|json)$|$).+)",
      "destination": "/:path/",
      "permanent": true
    },
    {
      "source": "/job",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/job/",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/job/index.html",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/registration",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/registration/",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/registration/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap-index.xml"
    }
  ]
}

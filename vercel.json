{
  "redirects": [
    {
      "source": "/:path*/index.html",
      "destination": "/:path*/",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path((?!index$|index\\.html$|.*\\/|$).+)",
      "destination": "/:path/",
      "permanent": true
    },
    {
      "source": "/job",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/job/",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/job/index.html",
      "destination": "/jobs/",
      "permanent": true
    },
    {
      "source": "/registration",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/registration/",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/registration/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap-index.xml"
    }
  ]
}
